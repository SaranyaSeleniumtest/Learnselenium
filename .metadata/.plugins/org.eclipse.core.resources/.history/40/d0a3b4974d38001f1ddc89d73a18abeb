package Interviewcode;

public class LearnPagination {

	static void string() {
		String txt = "426pages";
		String[] split = txt.split("pages");
		int length = split.length;
		System.out.println(length);
	System.out.println(split[1]);
		
	}
	
	public static void main(String[] args) {
		string();
	}
}
